{
    "name": "gsmanager-cloud/bus",
    "description": "The GSManager Bus package.",
    "license": "MIT",
    "homepage": "https://gsmanager.ru",
    "support": {
        "issues": "https://github.com/gsmanager-cloud/framework/issues",
        "source": "https://github.com/gsmanager-cloud/framework"
    },
    "authors": [
        {
            "name": "Sergey Tyuhtin",
            "email": "info@gsmanager.ru"
        }
    ],
    "require": {
        "php": "^8.2",
        "gsmanager-cloud/collections": "v1",
        "gsmanager-cloud/contracts": "v1",
        "gsmanager-cloud/pipeline": "v1",
        "gsmanager-cloud/support": "v1"
    },
    "autoload": {
        "psr-4": {
            "GSManager\\Bus\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "suggest": {
        "gsmanager-cloud/queue": "Required to use closures when chaining jobs (^1)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
