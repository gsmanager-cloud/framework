{
    "name": "gsmanager-cloud/database",
    "description": "The GSManager Database package.",
    "license": "MIT",
    "homepage": "https://gsmanager.ru",
    "support": {
        "issues": "https://github.com/gsmanager/framework/issues",
        "source": "https://github.com/gsmanager/framework"
    },
    "keywords": ["gsmanager", "database", "sql", "orm"],
    "authors": [
        {
            "name": "Sergey Tyuhtin",
            "email": "info@gsmanager.ru"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-pdo": "*",
        "brick/math": "^0.11|^0.12|^0.13",
        "gsmanager-cloud/collections": "dev-main",
        "gsmanager-cloud/container": "dev-main",
        "gsmanager-cloud/contracts": "dev-main",
        "gsmanager-cloud/macroable": "dev-main",
        "gsmanager-cloud/support": "dev-main",
        "gsmanager-cloud/serializable-closure": "^1.3|^2.0"
    },
    "autoload": {
        "psr-4": {
            "GSManager\\Database\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "suggest": {
        "ext-filter": "Required to use the Postgres database driver.",
        "fakerphp/faker": "Required to use the eloquent factory builder (^1.24).",
        "gsmanager-cloud/console": "Required to use the database commands (^1).",
        "gsmanager-cloud/events": "Required to use the observers with Eloquent (^1).",
        "gsmanager-cloud/filesystem": "Required to use the migrations (dev-main).",
        "gsmanager-cloud/http": "Required to convert Eloquent models to API resources (dev-main).",
        "gsmanager-cloud/pagination": "Required to paginate the result set (dev-main).",
        "symfony/finder": "Required to use Eloquent model factories (^7.2)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
