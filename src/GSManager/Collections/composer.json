{
    "name": "gsmanager-cloud/collections",
    "description": "The GSManager Collections package.",
    "license": "MIT",
    "homepage": "https://gsmanager.ru",
    "support": {
        "issues": "https://github.com/gsmanager/framework/issues",
        "source": "https://github.com/gsmanager/framework"
    },
    "authors": [
        {
            "name": "Sergey Tyuhtin",
            "email": "info@gsmanager.ru"
        }
    ],
    "require": {
        "php": "^8.2",
        "gsmanager-cloud/conditionable": "dev-main",
        "gsmanager-cloud/contracts": "dev-main",
        "gsmanager-cloud/macroable": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "GSManager\\Support\\": ""
        },
        "files": [
            "functions.php",
            "helpers.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "suggest": {
        "gsmanager-cloud/http": "Required to convert collections to API resources (dev-main).",
        "symfony/var-dumper": "Required to use the dump method (^7.2)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
