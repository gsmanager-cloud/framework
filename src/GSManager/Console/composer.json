{
    "name": "gsmanager-cloud/console",
    "description": "The GSManager Console package.",
    "license": "MIT",
    "homepage": "https://gsmanager.ru",
    "support": {
        "issues": "https://github.com/gsmanager-cloud/framework/issues",
        "source": "https://github.com/gsmanager-cloud/framework"
    },
    "authors": [
        {
            "name": "Sergey Tyuhtin",
            "email": "info@gsmanager.ru"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-mbstring": "*",
        "gsmanager-cloud/collections": "v1",
        "gsmanager-cloud/contracts": "v1",
        "gsmanager-cloud/macroable": "v1",
        "gsmanager-cloud/support": "v1",
        "gsmanager-cloud/view": "v1",
        "gsmanager-cloud/prompts": "v1",
        "nunomaduro/termwind": "^2.0",
        "symfony/console": "^7.2.0",
        "symfony/polyfill-php83": "^1.31",
        "symfony/process": "^7.2.0"
    },
    "autoload": {
        "psr-4": {
            "GSManager\\Console\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "suggest": {
        "ext-pcntl": "Required to use signal trapping.",
        "dragonmantank/cron-expression": "Required to use scheduler (^3.3.2).",
        "guzzlehttp/guzzle": "Required to use the ping methods on schedules (^7.8).",
        "gsmanager-cloud/bus": "Required to use the scheduled job dispatcher (^1).",
        "gsmanager-cloud/container": "Required to use the scheduler (^1).",
        "gsmanager-cloud/filesystem": "Required to use the generator command (v1).",
        "gsmanager-cloud/queue": "Required to use closures for scheduled jobs (v1)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
