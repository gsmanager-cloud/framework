{
    "name": "gsmanager-cloud/redis",
    "description": "The GSManager Redis package.",
    "license": "MIT",
    "homepage": "https://gsmanager.ru",
    "support": {
        "issues": "https://github.com/gsmanager/framework/issues",
        "source": "https://github.com/gsmanager/framework"
    },
    "authors": [
        {
            "name": "Sergey Tyuhtin",
            "email": "info@gsmanager.ru"
        }
    ],
    "require": {
        "php": "^8.2",
        "gsmanager-cloud/collections": "dev-main",
        "gsmanager-cloud/contracts": "dev-main",
        "gsmanager-cloud/macroable": "dev-main",
        "gsmanager-cloud/support": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "GSManager\\Redis\\": ""
        }
    },
    "suggest": {
        "ext-redis": "Required to use the phpredis connector (^4.0|^5.0|^6.0).",
        "predis/predis": "Required to use the predis connector (^2.3|^3.0)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
