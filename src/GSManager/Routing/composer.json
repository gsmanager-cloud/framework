{
    "name": "gsmanager-cloud/routing",
    "description": "The GSManager Routing package.",
    "license": "MIT",
    "homepage": "https://gsmanager.ru",
    "support": {
        "issues": "https://github.com/gsmanager-cloud/framework/issues",
        "source": "https://github.com/gsmanager-cloud/framework"
    },
    "authors": [
        {
            "name": "Sergey Tyuhtin",
            "email": "info@gsmanager.ru"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-filter": "*",
        "ext-hash": "*",
        "gsmanager-cloud/collections": "v1",
        "gsmanager-cloud/container": "v1",
        "gsmanager-cloud/contracts": "v1",
        "gsmanager-cloud/http": "v1",
        "gsmanager-cloud/macroable": "v1",
        "gsmanager-cloud/pipeline": "v1",
        "gsmanager-cloud/session": "v1",
        "gsmanager-cloud/support": "v1",
        "symfony/http-foundation": "^7.2.0",
        "symfony/http-kernel": "^7.2.0",
        "symfony/routing": "^7.2.0"
    },
    "autoload": {
        "psr-4": {
            "GSManager\\Routing\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "suggest": {
        "gsmanager-cloud/console": "Required to use the make commands (v1).",
        "php-http/discovery": "Required to use PSR-7 bridging features (^1.15).",
        "symfony/psr-http-message-bridge": "Required to use PSR-7 bridging features (^7.2)."
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": false
        }
    },
    "minimum-stability": "dev"
}
